{
    "name": "flatgeographbuf",
    "version": "1.0.0",
    "description": "A performant binary encoding for geospatial graphs, extending FlatGeobuf with adjacency list support",
    "scripts": {
        "dev": "vite",
        "build": "rm lib -rf && swc ./src/ts -d lib/mjs && mv ./lib/mjs/src/ts/* ./lib/mjs -f && rm ./lib/mjs/src -rf && tsc --emitDeclarationOnly && rollup -c rollup.config.js",
        "type-check": "tsc --noEmit",
        "test": "vitest ./lib",
        "test-src": "vitest ./src/ts",
        "coverage": "vitest run --coverage",
        "typedoc": "typedoc"
    },
    "files": [
        "lib/**/*",
        "dist/**/*"
    ],
    "types": "lib/mjs/flatgeographbuf.d.ts",
    "module": "lib/mjs/flatgeographbuf.js",
    "main": "lib/mjs/flatgeographbuf.js",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flatgeobuf/flatgeographbuf.git"
    },
    "keywords": [
        "spatial",
        "geometry",
        "feature",
        "flatbuffers",
        "serialization",
        "binary",
        "performance",
        "graph",
        "adjacency-list",
        "geospatial",
        "network"
    ],
    "author": "Cristiano Costa <cristiano@nexatlas.com>",
    "contributors": [
        "Cristiano Costa <cristiano@nexatlas.com>",
        "Joel Torres <joel.torres@nexatlas.com>",
        "BjÃ¶rn Harrtell (FlatGeobuf)"
    ],
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/flatgeobuf/flatgeographbuf/issues"
    },
    "homepage": "https://github.com/flatgeobuf/flatgeographbuf#readme",
    "dependencies": {
        "@repeaterjs/repeater": "3.0.6",
        "flatbuffers": "25.9.23",
        "slice-source": "0.4.1"
    },
    "optionalDependencies": {
        "ol": ">=10"
    },
    "devDependencies": {
        "@babel/core": "7.28.5",
        "@babel/preset-env": "7.28.5",
        "@biomejs/biome": "2.3.10",
        "@rollup/plugin-babel": "6.1.0",
        "@rollup/plugin-node-resolve": "16.0.3",
        "@rollup/plugin-terser": "0.4.4",
        "@swc/cli": "0.7.9",
        "@swc/core": "1.15.7",
        "@types/geojson": "7946.0.16",
        "@types/node": "25.0.3",
        "@types/slice-source": "0.4.3",
        "@vitest/coverage-v8": "4.0.16",
        "jsts": "2.12.1",
        "ol": ">=10",
        "rollup": "4.53.5",
        "sirv": "^3.0.2",
        "typedoc": "0.28.15",
        "typescript": "5.9.3",
        "vite": "7.3.0",
        "vitest": "4.0.16"
    }
}
